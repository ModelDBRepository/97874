proc colors () {}

proc crescale () {
  $o1.scale($2,$3)
  $o1.exec_menu("Space Plot")
  $o1.exec_menu("Shape Plot")
  $o1.flush
}

//* cbin(min,max): binary colormap
proc cbin () {
  $o1.colormap(2)
  $o1.colormap(0, 255, 0, 0)
  $o1.colormap(1, 255, 255, 0)
  $o1.scale($2, $3)
}

//* ternary color map
proc ctern () {
  $o1.colormap(3)
  $o1.colormap(0, 255, 0, 0)
  $o1.colormap(1, 255, 255, 0)
  $o1.colormap(2, 0, 0, 255)
  $o1.scale($2, $3)
}

//* cbw(min,max): b/w binary colormap
proc cbw () {
  $o1.colormap(2)
  $o1.colormap(0, 0, 0, 0)
  $o1.colormap(1, 255, 255, 255)
  $o1.scale($2, $3)
}

